{
  "token_hmac_secret_key": "your_secret_key",
  "api_key": "your_api_key",
  "admin_password": "admin_password",
  "admin_secret": "admin_secret",
  "allowed_origins": [
    "*"
  ],
  "log_level": "debug",
  "namespaces": [
    {
      "name": "world",
      "presence": true,
      "history_size": 100,
      "history_ttl": "300s",
      "force_recovery": true,
      "publish": false,
      "proxy_publish": false
    }
  ],
  "proxy_http_headers": [
    "User-Agent",
    "Cookie",
    "Authorization",
    "TestHeader",
    "X-Real-Ip",
    "X-Forwarded-For",
    "X-Request-Id"
  ],
  "proxy_connect_endpoint": "http://host.docker.internal:8000/api/adapters/centrifugo/connect/",
  "proxy_publish_endpoint": "http://host.docker.internal:8000/api/adapters/centrifugo/publish/",
  "proxy_subscribe_endpoint": "http://host.docker.internal:8000/api/adapters/centrifugo/subscribe/",
  "port": "8050",
  "internal_port": "8060"
}
